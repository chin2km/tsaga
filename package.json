{
  "name": "tsaga",
  "version": "2.3.0",
  "description": "Typesafe and lightweight way to write functions with asynchronous side-effects that are easily testable.",
  "main": "compiled/lib/index.js",
  "types": "compiled/lib/index.d.ts",
  "repository": "https://github.com/tp/tsaga.git",
  "author": "Timm Preetz <timm@preetz.us>",
  "license": "(MIT OR Apache-2.0)",
  "devDependencies": {
    "@types/jest": "^24",
    "@types/lodash": "^4.14.109",
    "@types/nock": "^9.1.3",
    "@types/node-fetch": "^2.1.1",
    "jest": "^24",
    "lodash": "^4.17.10",
    "nock": "^10",
    "node-fetch": "^2.1.2",
    "prettier": "^1.16.4",
    "redux": "^4.0.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24",
    "tslint": "^5.10.0",
    "typescript": "3.4.1"
  },
  "peerDependencies": {
    "redux": "4.x"
  },
  "scripts": {
    "watch:tsc": "rimraf compiled/ && tsc --watch",
    "watch:jest": "jest --watchAll",
    "lint": "tslint -c tslint.json --project .",
    "ts-check": "tsc -w --noEmit",
    "test": "prettier --list-different \"src/**/*.{ts,tsx}\" && tsc --noEmit && yarn lint && jest",
    "test:updateSnapshots": "jest --updateSnapshot",
    "prepublishOnly": "rimraf compiled/ && tsc"
  },
  "dependencies": {
    "expect": "^24.7.1",
    "typescript-fsa": "^2.5"
  },
  "files": [
    "compiled/lib/**/*"
  ]
